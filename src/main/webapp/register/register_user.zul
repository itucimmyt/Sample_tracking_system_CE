	<fisheye id="idLabFis"  onClick="idWindowRegister.add()" image="${imgNext}"/>
	<fisheye label="${lblMenTCancel}" image="${imgClose}" onClick="idWindowRegister.closeWindow()"/>
				 		</fisheyebar>
</window>
</zk>